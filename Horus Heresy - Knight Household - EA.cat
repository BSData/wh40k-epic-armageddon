    <rule id="3782-b9e0-2f32-148c" name="Aspirants" hidden="false">
      <profiles/>
      <rules/>
      <infoLinks/>
      <modifiers>
        <modifier type="set" field="description" value="&lt;I&gt;&lt;B&gt;Les Aspirants:&lt;/B&gt; La formation souffre -1  les jets d&apos;actions de Tir Soutenu, tat d&apos;Alerte, Redploiement, Regroupement et l&apos;Avance.&lt;/I&gt;">
