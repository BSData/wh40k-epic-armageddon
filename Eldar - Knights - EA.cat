        <modifier type="set" field="description" value="Eldar Knights Charaktere: Eine Armee der Eldar Knights kann so viele Individuen umfassen, wie es Knight Hosts oder Guardian Warhosts in der Armee gibt.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="8220-3d37-43f7-71e8" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>The number of Individuals may not exceed the number of Hosts in the army.</description>
    </rule>
