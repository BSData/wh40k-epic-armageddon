          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Schwere Konstrukt: Betrachtet unbefahrbares Gelände als gefährliches Gelände.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="8220-3d37-43f7-71e8" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Construcción Pesada: Considera que el terreno infranqueable es un terreno peligroso.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="3715-341b-881c-9c4c" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>Treats Impassible terrain as Dangerous terrain.</description>
    </rule>
    <rule id="e4a4-f170-fc42-a757" name="Critical Hit: Seraptek" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Touche Critique: Seraptek: Perte des deux Générateurs de Singularité. Les touches critiques ultérieures détruisent l&apos;unité.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="798a-d9b8-47b6-c74c" type="equalTo"/>
