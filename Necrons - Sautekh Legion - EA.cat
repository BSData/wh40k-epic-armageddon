        <characteristic characteristicId="affeeb83-c6db-0a8c-e7ab-688f9a83d25c" name="CC" value="2+"/>
        <characteristic characteristicId="0b1a118d-1f21-3fd1-9167-471a3032e35b" name="FF" value="4+"/>
        <characteristic characteristicId="1160-63e7-57ca-722d" name="Weapons" value="3x Scarab Swarm&lt;BR&gt;and&lt;BR&gt;or&lt;BR&gt;&lt;BR&gt;Harvesters"/>
        <characteristic characteristicId="02c3b688-b804-6ce1-b101-b7f9436877bf" name="Range" value="30cm&lt;BR&gt;and (15cm)&lt;BR&gt;or (contact)&lt;BR&gt;&lt;BR&gt;(contact)"/>
