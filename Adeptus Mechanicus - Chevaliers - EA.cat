    <rule id="8344-8828-daf3-9f1d" name="Alliance: Mechanicum" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Alliance : Arme du Mechanicum: Un tiers des points dune liste de Chevalier peut tre slectionn dans une des trois listes Taghmata Omnissiah ou Legio Titanicus ou de lAdeptus Mechanicus. Cette slection doit respecter la structure de ces listes.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="798a-d9b8-47b6-c74c" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Allianz: Mechanicum: Ein Drittel der Punkte einer Chevalierliste kann aus einer der drei Listen Legio Titanicus, Taghmata Omnissiah oder Adeptus Mechanicus ausgewhlt werden. Diese Auswahl muss die Struktur dieser Listen respektieren.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="8220-3d37-43f7-71e8" type="equalTo"/>
          </conditions>
        </modifier>
        <modifier type="set" field="description" value="Alianza: Mechanicum: Un tercio de los puntos de una lista de Chevaliers puede ser seleccionado de una de las tres listas de Legio Titanicus, Taghmata Omnissiah o Adeptus Mechanicus. Esta seleccin debe respetar la estructura de estas listas.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="3715-341b-881c-9c4c" type="equalTo"/>
          </conditions>
        </modifier>
      </modifiers>
      <description>One-third of the points in a Chevaliers list can be selected from one of the three Legio Titanicus, Taghmata Omnissiah, or Adeptus Mechanicus lists. This selection must respect the structure of these lists.</description>
    </rule>
    <rule id="fbc2-c589-3988-c74d" name="Chivalrous Courage" hidden="false">
      <modifiers>
        <modifier type="set" field="description" value="Courage Chevaleresque: Les formations de Chevaliers bnficient d&apos;un bonus de +1 lors des actions d&apos;assaut et de ralliement.">
          <conditions>
            <condition field="selections" scope="force" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="true" childId="798a-d9b8-47b6-c74c" type="equalTo"/>
          </conditions>
